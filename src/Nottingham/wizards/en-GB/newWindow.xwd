<wizard menus="Navigators">  <pageWizard remove="true" duplicate="false">    <newNodes>      <newWindow><![CDATA[<newWindow name="Enter Page Title" text="Enter text for the page" url="Enter a URL to launch" descrip="Enter link text" previewImage="Select a preview image" tip="Enter a Description for Accessibility" windowWidth="550" windowHeight="400"/>]]></newWindow>    </newNodes>  </pageWizard>  <!-- NEW WINDOW=================== -->  <newWindow menu="Navigators" menuItem="New Window" hint="Allow the user to launch a new window to a URL." icon="icApplication" thumb="thumbs/newWindow.jpg" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Page Text" type="TextArea" height="150"/>    <url label="URL" type="TextInput"/>	<descrip label="Link Text" type="TextInput"/>    <previewImage label="Preview Image" type="media"/>    <tip label="Image Description" type="TextInput"/>    <windowWidth label="Window Width" type="NumericStepper" min="100" max="980" step="10" defaultValue="550" width="100"/>    <windowHeight label="Window Height" type="NumericStepper" min="100" max="720" step="10" defaultValue="400" width="100"/>    <htm label="HTML" type="textArea" height="150" optional="true"/>  </newWindow></wizard>