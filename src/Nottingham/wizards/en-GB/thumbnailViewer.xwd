<wizard menus="Navigators">  <pageWizard remove="true" duplicate="false">    <newNodes>      <thumbnailViewer><![CDATA[<thumbnailViewer name="Enter Page Title" text="Enter text for the page" nextBtnTip="Next" priorBtnTip="Previous"/>]]></thumbnailViewer>    </newNodes>  </pageWizard>  <!-- THUMBNAIL VIEWER======================= -->  <thumbnailViewer menu="Navigators" menuItem="Thumbnail Viewer" hint="A series of thumbnail images for the user to browse." icon="icPictures" thumb="thumbs/thumb.jpg" remove="true">        <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Text" type="TextArea" height="250"/>		<showTitles label="Show Image Titles" type="CheckBox" defaultValue="false" optional="true"/>	    <nextBtnTip label="Next Button Tip" type="textInput" divider="true" language="true" />	    <priorBtnTip label="Prior Button Tip" type="textInput" language="true" />    <newNodes>      <thumbnailImage><![CDATA[<thumbnailImage name="name" text="Content for the pane" align="Top" url="" linktarget="none" link="" tip="Enter a Description for Accessibility"/>]]></thumbnailImage>    </newNodes>  </thumbnailViewer>    <thumbnailImage menuItem="Image" icon="icBullet" remove="true">    <name label="Image Title" type="TextInput" wysiwyg="true" />    <text label="Image Text" type="TextArea" height="250" />	<linktarget label="Link from image" options="None,Original image,Specified URL" type="ComboBox"  data="None,Original image,Specified URL" defaultValue="None" width="200"/>    <link label="Specified URL" type="textInput"/>	      <url label="Image" type="media"/>    <tip label="Image Description" type="TextInput" mandatory="true" defaultValue="Enter a Description for Accessibility" />  </thumbnailImage>  </wizard>